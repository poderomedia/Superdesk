<tfoot>
<tr>
	<td colspan="3">
	{?pagination}
	<ul class="the-icons unstyled">
		{#pagination}
		<li>
			<a class="previous" href="{prev}" {prevElement}>
				<i class="icon-chevron-left"></i>
			</a>
		</li>
		<li>{offset} - {range} of {total}</li>
		<li>
			<a class="next" href="{next}" {nextElement}>
				<i class="icon-chevron-right"></i>
			</a>
		</li>
		{/pagination}
	</ul>
	{/pagination}
	</td>
</tr>
</tfoot>
